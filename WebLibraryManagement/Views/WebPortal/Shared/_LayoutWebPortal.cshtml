    <!-- BEGIN: PAGE STYLES -->
    <link href="/Content/webportal/assets/plugins/ilightbox/css/ilightbox.css" rel="stylesheet" type="text/css" />
    <!-- END: PAGE STYLES -->
    <!-- BEGIN THEME STYLES -->
    <link href="/Content/webportal/css/plugins.css" rel="stylesheet" type="text/css" />
    <link href="/Content/webportal/css/components.css" id="style_components" rel="stylesheet" type="text/css" />
    <link href="/Content/webportal/css/default.css" rel="stylesheet" id="style_theme" type="text/css" />
    <link href="/Content/webportal/css/custom.css" rel="stylesheet" type="text/css" />
    <!-- END THEME STYLES -->
    <link rel="icon" href="~/Content/webportal/favicon.ico" type="image/x-icon">

    <!--[if lt IE 9]>
    <script src="../../assets/global/plugins/excanvas.min.js"></script>
    <![endif]-->
    <script src="/Content/webportal/assets/plugins/jquery.min.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/plugins/jquery-migrate.min.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/plugins/jquery.easing.min.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/plugins/reveal-animate/wow.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/demos/index/js/scripts/reveal-animate/reveal-animate.js" type="text/javascript"></script>
