    <script src="~/Content/js/core.services.js"></script>
    <script src="~/Content/js/core.format.services.js"></script>
    <!-- END: PAGE SCRIPTS -->
    <!-- END: LAYOUT/BASE/BOTTOM -->
    @RenderSection("scripts", required: false)
</head>
<body>
    <!-- ======= Header ======= -->
    @Html.Partial(Constant.PartialView_HeaderWebPortalPartial)
    <!-- End Header -->
    <!-- ======= Hero Section ======= -->
    @*@Html.Partial(Constant.PartialView_SlideWebPortalPartial)*@
    <!-- End Hero -->
    <div class="c-layout-page">
        @Html.Partial(Constant.PartialView_LoadingPartial)
        @RenderBody()
    </div>
    <!-- End #main -->
    <!-- ======= Footer ======= -->
    @Html.Partial(Constant.PartialView_FooterWebPortalPartial)
    <!-- End Footer -->
    <!-- END: LAYOUT/FOOTERS/FOOTER-6 -->
    <!-- BEGIN: LAYOUT/FOOTERS/GO2TOP -->
    <div class="c-layout-go2top">
        <i class="icon-arrow-up"></i>
    </div>
    <!-- END: LAYOUT/FOOTERS/GO2TOP -->
    <!-- BEGIN: LAYOUT/BASE/BOTTOM -->
    <!-- BEGIN: CORE PLUGINS -->

</body>

</html>