    <script src="/Content/webportal/assets/plugins/slider-for-bootstrap/js/bootstrap-slider.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/plugins/js-cookie/js.cookie.js" type="text/javascript"></script>
    @Scripts.Render("~/bundles/select2full")
    @Scripts.Render("~/bundles/componentsselect2")
    @Scripts.Render("~/bundles/bootstrapdatepicker")
    <!-- END: LAYOUT PLUGINS -->
    <!-- BEGIN: THEME SCRIPTS -->
    <script src="/Content/webportal/assets/base/js/components.js" type="text/javascript"></script>
    <script src="/Content/webportal/assets/base/js/components-shop.js" type="text/javascript"></script>
    @*<script src="/Content/assets/global/scripts/app.min.js"></script>*@
    <script src="/Content/webportal/assets/base/js/app.js" type="text/javascript"></script>
    <script>
        $(document).ready(function () {
            App.init(); // init core
        });
    </script>
    <!-- END: THEME SCRIPTS -->
    <!-- BEGIN: PAGE SCRIPTS -->
    <script src="/Content/webportal/assets/demos/default/js/scripts/revo-slider/slider-4.js" type="text/javascript"></script>
